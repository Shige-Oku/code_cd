   ],
   "source": [
    "Hn = list(map(int, input().split()))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "aad0d4e0-6824-4689-ba80-2f870ff3e9f4",
   "metadata": {},
   "outputs": [],
   "source": [
    "dp = [10**10] * N\n",
    "dp[0] = 0\n",
    "for i in range(1, N):\n",
    "    dp[i] = dp[i-1] + abs(Hn[i] - Hn[i-1])\n",
    "    if i > 1:\n",
    "        dp[i] = min(dp[i], dp[i-2] + abs(Hn[i] - Hn[i-2]))"
   ]
  },
