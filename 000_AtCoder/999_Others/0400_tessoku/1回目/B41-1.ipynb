   "outputs": [],
   "source": [
    "op = []\n",
    "x, y = X, Y\n",
    "while x > 1 or y > 1:\n",
    "    op.append((x, y))\n",
    "    if x > y:\n",
    "        x -= y\n",
    "    else:\n",
    "        y -= x\n"
   ]
