   ],
   "source": [
    "ans = 10 ** 20\n",
    "for i in range(1, math.ceil(math.sqrt(N))+1):\n",
    "    q, m = divmod(N, i)\n",
    "    if m  != 0:\n",
    "        continue\n",
    "    ans = min(ans, abs(i-1)+(q-1))\n"
   ]
